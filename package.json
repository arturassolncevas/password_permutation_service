    "@babel/register": "^7.12.1",
    "babel-loader": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "file-loader": "^6.2.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "chokidar": "^3.4.3",
